local Players = game:GetService("Players")
local LogService = game:GetService("LogService")

local knownExecutors = {
    "ExecutorName1",
    "ExecutorName2",
    "ExecutorName3"
}

local function kickPlayer(player, reason)
    player:Kick(reason)
end

Players.PlayerAdded:Connect(function(player)
    -- Cek nama executor di log
    LogService.MessageOut:Connect(function(message)
        for _, executorName in ipairs(knownExecutors) do
            if string.find(message, executorName) then
                kickPlayer(player, "Akun anda telah di banned oleh moderator roblox.")
            end
        end
    end)
end)
